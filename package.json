{
  "name": "mobile-insurance",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "standard",
    "start": "webpack-dev-server --config webpack.config.js --progress --hot",
    "build": "npm run buildClean  &&  webpack  --mode production --progress --profile",
