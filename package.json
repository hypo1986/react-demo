    "babel-plugin-import": "^1.8.0",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-polyfill": "^6.26.0",
    "babel-preset-stage-2": "^6.24.1",
    "cache-loader": "^1.2.2",
    "eslint": "^5.2.0",
    "eslint-config-standard": "^11.0.0",
